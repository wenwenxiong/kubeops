# ## Configure 'ip' variable to bind kubernetes services on a
# ## different ip than the default iface
kubespray01 ansible_ssh_host=192.168.122.61 ansible_user=root ansible_ssh_private_key_file="/root/ansible/kismatic/cluster00/kismaticuser.key"
kubespray02 ansible_ssh_host=192.168.122.62 ansible_user=root ansible_ssh_private_key_file="/root/ansible/kismatic/cluster00/kismaticuser.key"


# ## We should set etcd_member_name for etcd cluster. The node that is not a etcd member do not need to set the value, or can set the empty string value.
# kubespray01 ansible_host=95.54.0.12  # ip=10.3.0.1 etcd_member_name=etcd1
# kubespray02 ansible_host=95.54.0.13  # ip=10.3.0.2 etcd_member_name=etcd2
# node3 ansible_host=95.54.0.14  # ip=10.3.0.3 etcd_member_name=etcd3
# node4 ansible_host=95.54.0.15  # ip=10.3.0.4 etcd_member_name=etcd4
# node5 ansible_host=95.54.0.16  # ip=10.3.0.5 etcd_member_name=etcd5
# node6 ansible_host=95.54.0.17  # ip=10.3.0.6 etcd_member_name=etcd6

# ## configure a bastion host if your nodes are not directly reachable
# bastion ansible_host=x.x.x.x ansible_user=some_user

# [kube-master]
# kubespray01
# kubespray02
[kube-master]
kubespray01
kubespray02

# [etcd]
# kubespray01
# kubespray02
# node3

[etcd]
kubespray01

# [kube-node]
# kubespray02
# node3
# node4
# node5
# node6
[kube-node]
kubespray02
kubespray01

# [k8s-cluster:children]
# kube-master
# kube-node
[k8s-cluster:children]
kube-master
kube-node
